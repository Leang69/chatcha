{
    "openapi": "3.0.0",
    "info": {
        "title": "ChatCha Backend API",
        "version": "1.0.0"
    },
    "paths": {
        "/signup": {
            "post": {
                "tags": [
                    "auth"
                ],
                "operationId": "authLogout",
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "401": {
                        "description": "Returns when user is not authenticated",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Not authorized"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "security": [
        []
    ]
}